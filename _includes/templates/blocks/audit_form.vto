<form action="https://prospects.getbadger.io/reports" method="post" class="block-auditForm {{ block.class }}" target="_blank">
  <div class="field has-cta">
    <label for="url" class="field-label">{{ block.field.label }}</label>
    <input type="url" class="input field-input" name="url" id="url" placeholder={{ block.field.placeholder }}>
    <div class="field-cta">
      <button class="button is-primary">{{ block.cta.text }}</button>
    </div>
  </div>
</form>